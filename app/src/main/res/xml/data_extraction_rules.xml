<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Include user profile and gamification data for backup -->
        <include domain="sharedpref" path="gamification_prefs"/>
    </cloud-backup>

    <device-transfer>
        <!-- Include user profile and gamification data for device transfer -->
        <include domain="sharedpref" path="gamification_prefs"/>
    </device-transfer>
</data-extraction-rules>